generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Profile {
  id           String   @id @default(cuid())
  clerkUserId  String   @unique
  email        String?
  fullName     String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
